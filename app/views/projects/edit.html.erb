<div id="back-to-project-link" class="link-underline">
  <i class="fa fa-arrow-left" aria-hidden="true"></i>
  <%= link_to "Back to my project", project_path(@project) %>
</div>
<h2 class="bold-primary">Project Settings</h2>
<button id="project-complete-button" class="big-bold-button btn btn-success" data-toggle="modal" data-target="#exampleModal">Complete!</button>
<%= render "form" %>
<%= render "complete_project_modal" %>

<%= content_for :javascript do %>
  <%= javascript_tag  do %>
    <%= raw render "slider.js.erb" %>
  <% end %>
<% end %>
