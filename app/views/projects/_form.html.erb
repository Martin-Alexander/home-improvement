<%= simple_form_for @project, html: { class: "projects-form", id: "projects-form" } do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :private %>
  <%= f.input :status %>
  <%= f.input :estimated_level_of_effort %>
  <%= f.input :actual_level_of_effort, as: :hidden %>
  <%= f.button :submit %>
<% end %>